[
  {
    "dnsSearchDomains": null,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${log_group}",
        "awslogs-region": "${region}",
        "awslogs-stream-prefix": "${log_stream_prefix}"
      }
    },
    "entryPoint": null,
    "portMappings": [
      {
        "hostPort": 0,
        "protocol": "tcp",
        "containerPort": 3000
      }
    ],
    "command": null,
    "linuxParameters": null,
    "cpu": ${cpu},
    "environment": [
      {
        "name": "DATABASE_URL",
        "value": "${DATABASE_URL}"
      },
      {
        "name": "ADDR",
        "value": "0.0.0.0"
      },
      {
        "name": "PORT",
        "value": "3000"
      },
      {
        "name": "HOST",
        "value": "${HOST}"
      },
      {
        "name": "AWS_DEFAULT_REGION",
        "value": "${AWS_DEFAULT_REGION}"
      },
      {
        "name": "AWS_S3_BUCKET",
        "value": "${AWS_S3_BUCKET}"
      },
      {
        "name": "AWS_ACCESS_KEY_ID",
        "value": "${AWS_ACCESS_KEY_ID}"
      },
      {
        "name": "AWS_SECRET_ACCESS_KEY",
        "value": "${AWS_SECRET_ACCESS_KEY}"
      },
      {
        "name": "SUPPORT_EMAIL",
        "value": "${SUPPORT_EMAIL}"
      },
      {
        "name": "EMAIL_FROM_ADDRESS",
        "value": "${EMAIL_FROM_ADDRESS}"
      },
      {
        "name": "EMAIL_SERVICE",
        "value": "${EMAIL_SERVICE}"
      },
      {
        "name": "SESSION_SECRET",
        "value": "${SESSION_SECRET}"
      },
      {
        "name": "UI_URL",
        "value": "${UI_URL}"
      },
      {
        "name": "GO_ENV",
        "value": "${GO_ENV}"
      },
      {
        "name": "ROLLBAR_TOKEN",
        "value": "${ROLLBAR_TOKEN}"
      },
      {
        "name": "LOG_LEVEL",
        "value": "${LOG_LEVEL}"
      },
      {
        "name": "DISABLE_TLS",
        "value": "${DISABLE_TLS}"
      },
      {
        "name": "CERT_DOMAIN_NAME",
        "value": "${CERT_DOMAIN_NAME}"
      },
      {
        "name": "APP_NAME",
        "value": "${APP_NAME}"
      }
    ],
    "ulimits": null,
    "dnsServers": null,
    "mountPoints": [],
    "workingDirectory": null,
    "dockerSecurityOptions": null,
    "memory": ${memory},
    "memoryReservation": null,
    "volumesFrom": [],
    "image": "${docker_image}:${docker_tag}",
    "disableNetworking": null,
    "healthCheck": null,
    "essential": true,
    "links": null,
    "hostname": null,
    "extraHosts": null,
    "user": null,
    "readonlyRootFilesystem": null,
    "dockerLabels": null,
    "privileged": null,
    "name": "buffalo"
  }
]
