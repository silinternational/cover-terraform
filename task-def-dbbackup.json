[
  {
    "dnsSearchDomains": null,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${log_group_name}",
        "awslogs-region": "${aws_region}",
        "awslogs-stream-prefix": "${app_name}"
      }
    },
    "entryPoint": null,
    "portMappings": [],
    "command": [],
    "cpu": ${cpu},
    "environment": [
      {
        "name": "AWS_ACCESS_KEY",
        "value": "${aws_access_key}"
      },
      {
        "name": "AWS_SECRET_KEY",
        "value": "${aws_secret_key}"
      },
      {
        "name": "B2_APPLICATION_KEY_ID",
        "value": "${b2_application_key_id}"
      },
      {
        "name": "B2_APPLICATION_KEY",
        "value": "${b2_application_key}"
      },
      {
        "name": "B2_BUCKET",
        "value": "${b2_bucket}"
      },
      {
        "name": "B2_HOST",
        "value": "${b2_host}"
      },
      {
        "name": "DB_HOST",
        "value": "${db_host}"
      },
      {
        "name": "DB_NAME",
        "value": "${db_name}"
      },
      {
        "name": "DB_OPTIONS",
        "value": "${db_options}"
      },
      {
        "name": "MODE",
        "value": "${service_mode}"
      },
      {
        "name": "DB_ROOTPASSWORD",
        "value": "${db_rootpassword}"
      },
      {
        "name": "DB_ROOTUSER",
        "value": "${db_rootuser}"
      },
      {
        "name": "DB_USER",
        "value": "${db_user}"
      },
      {
        "name": "DB_USERPASSWORD",
        "value": "${db_userpassword}"
      },
      {
        "name": "S3_BUCKET",
        "value": "s3://${s3_bucket}"
      }
    ],
    "ulimits": null,
    "dnsServers": null,
    "mountPoints": [],
    "workingDirectory": null,
    "dockerSecurityOptions": null,
    "memory": null,
    "memoryReservation": ${memory},
    "volumesFrom": [],
    "image": "${docker_image}:${docker_tag}",
    "disableNetworking": null,
    "essential": true,
    "links": null,
    "hostname": null,
    "extraHosts": null,
    "user": null,
    "readonlyRootFilesystem": null,
    "dockerLabels": null,
    "privileged": null,
    "name": "backup"
  }
]
